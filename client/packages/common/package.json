{
  "name": "@eclipsesource/uml-glsp-common",
  "version": "1.0.0",
  "private": true,
  "description": "Common utilities",
  "keywords": [
    "theia-extension"
  ],
  "files": [
    "lib",
    "src"
  ],
  "scripts": {
    "build": "npx tsc -b && yarn run lint",
    "clean": "npx rimraf lib tsconfig.tsbuildinfo",
    "icons": "npx ts-node ./scripts/icons.ts",
    "lint": "npx eslint -c ./.eslintrc.js --ext .ts ./src",
    "prepare": "yarn run clean && yarn run build",
    "watch": "npx tsc -w"
  },
  "dependencies": {
    "@theia/core": "^1.34.0"
  },
  "devDependencies": {
    "endent": "^2.1.0"
  },
  "theiaExtensions": [
    {
      "frontend": "lib/browser/common-frontend-module"
    }
  ]
}
