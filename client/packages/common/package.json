{
  "private": true,
  "name": "@eclipsesource/uml-glsp-common",
  "version": "0.1.0",
  "description": "Common utilities",
  "keywords": [
    "theia-extension"
  ],
  "dependencies": {
    "@theia/core": "^1.0.0"
  },
  "devDependencies": {
    "rimraf": "^2.6.1",
    "typescript": "~4.5.5",
    "endent": "^2.1.0"
  },
  "scripts": {
    "prepare": "yarn run clean && yarn run build",
    "clean": "rimraf lib",
    "build": "tsc && yarn run lint",
    "lint": "eslint -c ./.eslintrc.js --ext .ts ./src",
    "watch": "tsc -w",
    "icons": "npx ts-node ./scripts/icons.ts"
  },
  "files": [
    "lib",
    "src"
  ],
  "theiaExtensions": [
    {
      "frontend": "lib/browser/common-frontend-module"
    }
  ]
}
