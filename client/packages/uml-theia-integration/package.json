{
  "name": "@eclipsesource/uml-theia-integration",
  "version": "1.0.0",
  "description": "Glue code to integrate UML GLSP sprotty client into Eclipse Theia",
  "keywords": [
    "theia-extension"
  ],
  "homepage": "https://github.com/eclipsesource/uml-glsp",
  "bugs": "https://github.com/eclipsesource/uml-glsp/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/eclipsesource/uml-glsp.git"
  },
  "license": "(EPL-2.0 OR MIT)",
  "author": {
    "name": "EclipseSource"
  },
  "files": [
    "lib",
    "src"
  ],
  "scripts": {
    "build": "tsc && yarn run lint",
    "clean": "rimraf lib tsconfig.tsbuildinfo",
    "lint": "eslint -c ./.eslintrc.js --ext .ts ./src",
    "prepare": "yarn run clean && yarn run build",
    "watch": "tsc -w"
  },
  "dependencies": {
    "@eclipse-emfcloud/modelserver-client": "next",
    "@eclipse-emfcloud/modelserver-theia": "next",
    "@eclipse-glsp/client": "next",
    "@eclipse-glsp/theia-integration": "next",
    "@eclipsesource/uml-common": "*",
    "@eclipsesource/uml-glsp": "*",
    "@eclipsesource/uml-modelserver": "*",
    "@theia/core": "next",
    "@theia/navigator": "next",
    "@theia/outline-view": "next",
    "sprotty-theia": "next"
  },
  "theiaExtensions": [
    {
      "frontend": "lib/browser/frontend.module",
      "backend": "lib/node/backend.module"
    }
  ]
}
