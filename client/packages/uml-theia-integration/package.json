    "@theia/navigator": "^1.34.0",
    "@theia/outline-view": "^1.34.0",
    "sprotty-theia": "^0.13.0-next.9cbedec"
  },
  "devDependencies": {},
  "theiaExtensions": [
    {
      "frontend": "lib/browser/uml-frontend-module",
      "backend": "lib/node/uml-backend-module"
    }
  ]
}
