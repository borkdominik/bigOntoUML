{
  "name": "theia-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "theia build --mode development",
    "clean": "theia clean",
    "download:plugins": "theia download:plugins",
    "prepare": "yarn build && yarn download:plugins",
    "start": "theia start --UML_GLSP=5007 --hostname=0.0.0.0 --root-dir=../workspace",
    "start:debug": "theia start --UML_GLSP=5007 --hostname=0.0.0.0 --loglevel=debug --root-dir=../../workspace --debug",
    "watch": "theia build --watch --mode development"
  },
  "dependencies": {
    "@eclipsesource/uml-glsp-common": "1.0.0",
    "@eclipsesource/uml-theia-integration": "1.0.0",
    "@theia/core": "^1.34.0",
    "@theia/editor": "^1.34.0",
    "@theia/file-search": "^1.34.0",
    "@theia/filesystem": "^1.34.0",
    "@theia/keymaps": "^1.34.0",
    "@theia/markers": "^1.34.0",
    "@theia/messages": "^1.34.0",
    "@theia/monaco": "^1.34.0",
    "@theia/navigator": "^1.34.0",
    "@theia/outline-view": "^1.34.0",
    "@theia/output": "^1.34.0",
    "@theia/plugin-ext": "^1.34.0",
    "@theia/plugin-ext-vscode": "^1.34.0",
    "@theia/plugin-metrics": "^1.34.0",
    "@theia/preferences": "^1.34.0",
    "@theia/process": "^1.34.0",
    "@theia/search-in-workspace": "^1.34.0",
    "@theia/task": "^1.34.0",
    "@theia/terminal": "^1.34.0",
    "@theia/userstorage": "^1.34.0",
    "@theia/variable-resolver": "^1.34.0",
    "@theia/vsx-registry": "^1.34.0",
    "@theia/workspace": "^1.34.0"
  },
  "devDependencies": {
    "@theia/cli": "^1.34.0"
  },
  "theia": {
    "target": "browser"
  },
  "theiaPlugins": {
    "vscode-builtin-json": "https://open-vsx.org/api/vscode/json/1.46.1/file/vscode.json-1.46.1.vsix",
    "vscode-builtin-json-language-features": "https://open-vsx.org/api/vscode/json-language-features/1.46.1/file/vscode.json-language-features-1.46.1.vsix"
  },
  "theiaPluginsDir": "plugins"
}
